{
  "expo": {
    "name": "sila",
    "slug": "sila",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "Bildirim için izniniz gerekiyor.",
        "NSContactsUsageDescription": "Rehber bilgilerinize erişmek istiyoruz."
      },
      "bundleIdentifier": "com.anonymous.sila"
    },
    "android": {
      "permissions": ["READ_CONTACTS", "NOTIFICATIONS"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appstore.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.sila"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts.",
          "contactsUsageDescription": "We need access to your contacts to provide better service."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "54751adc-2890-4da4-9d3c-6433e68c2460"
      }
    }
  }
}
